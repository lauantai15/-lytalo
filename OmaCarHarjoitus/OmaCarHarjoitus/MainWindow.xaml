<Window x:Class="OmaCarHarjoitus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OmaCarHarjoitus"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="36" Margin="68,62,0,0" VerticalAlignment="Top" Width="120" Click="button_Click"/>
        <TextBox x:Name="maxSpeedTextBox" HorizontalAlignment="Left" Height="24" Margin="68,131,0,0" TextWrapping="Wrap" Text="200" VerticalAlignment="Top" Width="120">
            <TextBox.ContextMenu>
                <ContextMenu/>
            </TextBox.ContextMenu>
        </TextBox>

    </Grid>
</Window>
